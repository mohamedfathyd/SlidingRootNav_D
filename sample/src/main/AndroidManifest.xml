<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yarolegovich.dalilegamalek.sample">
<uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.yarolegovich.dalilegamalek.sample.Activity.FilterActivity"></activity>
        <activity android:name="com.yarolegovich.dalilegamalek.sample.Activity.Profile_page" />
        <activity android:name="com.yarolegovich.dalilegamalek.sample.Activity.show" />
        <activity android:name="com.yarolegovich.dalilegamalek.sample.Activity.List_activity" />
        <activity
            android:name="com.yarolegovich.dalilegamalek.sample.Activity.Regestration_activity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateAlwaysHidden" />
        <activity
            android:name="com.yarolegovich.dalilegamalek.sample.Activity.Login_activity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateAlwaysHidden">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.yarolegovich.dalilegamalek.sample.SampleActivity"
            android:screenOrientation="portrait" />
    </application>

</manifest>